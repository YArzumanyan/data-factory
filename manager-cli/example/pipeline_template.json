{
  "title": "Article processing",
  "description": "This pipeline processes articles by validating and reversing the data.",
  "variables": [
    {
      "id": "article-var",
      "title": "Article Raw Input",
      "datasetUuid": "DATASET_UUID"
    },
    {
      "id": "validated-data-var",
      "title": "Intermediate Validated Data"
    },
    {
      "id": "reversed-output-var",
      "title": "Final Reversed Output Var"
    }
  ],
  "steps": [
    {
      "id": "validate-step",
      "title": "Validate Raw",
      "pluginUuid": "PLUGIN_UUID",
      "inputs": [
        "article-var"
      ],
      "outputs": [
        "validated-data-var"
      ]
    },
    {
      "id": "reverse-step",
      "title": "Reverse Data",
      "pluginUuid": "PLUGIN_UUID",
      "inputs": [
        "validated-data-var"
      ],
      "outputs": [
        "reversed-output-var"
      ],
      "precededBy": [
        "validate-step"
      ]
    }
  ]
}